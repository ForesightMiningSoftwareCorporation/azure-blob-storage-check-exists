  "devDependencies": {
    "@types/node": "^18.6.3",
    "@typescript-eslint/parser": "^5.41.0",
    "@vercel/ncc": "^0.34.0",
    "eslint": "^8.21.0",
    "eslint-plugin-github": "^4.3.2",
    "eslint-plugin-jest": "^26.7.0",
    "jest": "^28.1.3",
    "js-yaml": "^4.1.0",
    "prettier": "2.7.1",
    "ts-jest": "^28.0.7",
    "typescript": "^4.4.4"
